{
  "root": true,
  "extends": [
    "eslint:recommended",
    "fliplet",
    "plugin:vue/essential"
  ],
  "globals": {
    "Vue": true
  },
  "rules": {
    "no-new": 0,
    "space-before-function-paren": ["error", {
      "anonymous": "always",
      "named": "never",
      "asyncArrow": "always"
    }]
  }
}
